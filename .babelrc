{
    "plugins": [
        "dynamic-import-node-sync",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime",
        ["@babel/plugin-transform-react-jsx", { "pragma": "h" }]
    ],
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "node": "current",
                "browsers": ["last 2 versions"]
            }
        }]
    ]
}