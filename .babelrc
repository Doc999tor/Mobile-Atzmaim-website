{
  "sourceMaps": true,
  "presets": [
    ["@babel/preset-env", {
      "targets": {
          "node": "current",
          "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["@babel/plugin-transform-react-jsx", { "pragma": "h" }]
  ]
}
